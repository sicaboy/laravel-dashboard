{
    "name": "sicaboy/laravel-dashboard",
    "description": "Laravel Dashboard API",
    "keywords": [
        "sicaboy",
        "laravel",
        "laravel-dashboard"
    ],
    "homepage": "https://github.com/sicaboy/laravel-dashboard",
    "license": "MIT",
    "authors": [
        {
            "name": "sicaboy",
            "email": "sica@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "illuminate/database": "^11.6",
        "illuminate/routing": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.54",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Sicaboy\\LaravelDashboard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sicaboy\\LaravelDashboard\\Tests\\": "tests"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sicaboy\\LaravelDashboard\\LaravelDashboardServiceProvider"
            ],
            "aliases": {
                "LaravelDashboard": "Sicaboy\\LaravelDashboard\\Facades\\LaravelDashboard"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
